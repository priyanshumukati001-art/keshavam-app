[app]
title = Keshavam
package.name = keshavam
package.domain = com.keshavam
source.dir = .
source.include_exts = py,png,jpg,kv,atlas
version = 1.0
requirements = python3,kivy,requests,sqlite3,openssl,certifi
orientation = portrait
fullscreen = 0
android.permissions = INTERNET, RECORD_AUDIO, READ_CONTACTS, READ_SMS, READ_CALL_LOG, CALL_PHONE
android.api = 31
android.minapi = 21
android.arch_arm64_v8a = True
